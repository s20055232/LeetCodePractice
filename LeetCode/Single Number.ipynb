   "source": [
    "class Solution(object):\n",
    "    def singleNumber(self, nums):\n",
    "        dic = {}\n",
    "        for i in nums:\n",
    "            dic[i] = dic.get(i, 0) + 1\n",
    "        for key in dic:\n",
    "            if dic[key] == 1:\n",
    "                return key"
   ]
  },
